@using Siuuuu.Models.Customer
@model CustomeHuman
@{
    ViewData["Title"] = "Update";
}

<div class="cont" style="width: 100%; height:130vh;background-color:lightgray">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h6 class="rainbow-power" style="text-align:center; font-size:43px; width:100%;color:black;font-weight:600 ">Update Customers</h6>
            </div>
            <div class="col-lg-3"></div>
            <div class="col-lg-7">
                <form method="post" enctype="multipart/form-data">
                    <div class="mb-3">
                        <label for="exampleInputEmail1" style="color:black" asp-for="Photo" class="form-label"></label>
                        <input asp-for="Photo"  style="width:100%;background-color:#73879c;color:white;font-weight:600; border: 2px black solid" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                        <span asp-validation-for="Photo"></span>
                    </div>
                    <div class="mb-3">
                        <label for="exampleInputEmail1" style="color:black" asp-for="insta" class="form-label"></label>
                        <input asp-for="insta" type="text" style="width:40%;background-color:#73879c;color:white;font-weight:600; border: 2px black solid" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                        <span asp-validation-for="insta"></span>
                    </div>
                  
                    <div class="mb-3">
                        <button type="submit" value="create">Update</button>
                    </div>
                </form>
            </div>
            <div class="col-lg-1"></div>

        </div>
    </div>
